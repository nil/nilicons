{
  "name": "nilicons",
  "version": "0.1.7",
  "description": "My personal icon set",
  "main": "index.js",
  "scripts": {
    "build": "bash script/build.sh",
    "build:temp": "node script/temp.js",
    "build:icons": "svgr --config-file=.svgrrc.js --out-dir=./lib/icons \"temp-icons/*.svg\"",
    "build:data": "node script/data.js",
    "unzip": "unzip -o -d icons icons.zip && rm icons.zip",
    "svgo": "svgo --config=.svgo.yml -f icons -r",
    "babel": "babel ./lib/icons --out-dir ./",
    "prepublish": "rm lib/icons/index.js && node script/check.js",
    "postpublish": "node script/clean.js"
  },
  "author": "Nil Vila",
  "license": "MIT",
  "devDependencies": {
    "@svgr/cli": "^5.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "fs-extra": "^8.1.0",
    "jest": "^25.2.0",
    "lodash.merge": "^4.6.2",
    "svgo": "^1.3.2"
  }
}

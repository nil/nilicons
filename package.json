{
  "name": "nilicons",
  "version": "0.1.0",
  "description": "My personal icon set",
  "main": "index.js",
  "scripts": {
    "build": "node script/build.js --input icons/**/*.svg --output lib/build/data.json",
    "unzip": "unzip -o -d icons icons.zip && rm icons.zip",
    "svgo": "svgo --config=.svgo.yml -f icons -r",
    "short": "rm -rf icons &&npm run unzip && npm run svgo && npm run build"
  },
  "author": "Nil Vila",
  "license": "MIT",
  "devDependencies": {
    "cheerio": "^1.0.0-rc.3",
    "fs-extra": "^8.1.0",
    "globby": "^11.0.0",
    "lodash.merge": "^4.6.2",
    "shelljs": "^0.8.3",
    "svgo": "^1.3.2",
    "trim-newlines": "^3.0.0",
    "yargs": "^15.3.1"
  }
}

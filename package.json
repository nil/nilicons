{
  "name": "nilicons",
  "version": "0.1.2",
  "description": "My personal icon set",
  "main": "index.js",
  "scripts": {
    "build": "bash script/build.sh",
    "unzip": "unzip -o -d icons icons.zip && rm icons.zip",
    "svgo": "svgo --config=.svgo.yml -f icons -r",
    "svgr": "svgr --config-file=.svgrrc.js --out-dir=./ \"temp-icons/*.svg\"",
    "postpublish": "node script/clean.js"
  },
  "author": "Nil Vila",
  "license": "MIT",
  "devDependencies": {
    "@svgr/cli": "^5.3.0",
    "fs-extra": "^8.1.0",
    "lodash.merge": "^4.6.2",
    "shelljs": "^0.8.3",
    "svgo": "^1.3.2"
  }
}
